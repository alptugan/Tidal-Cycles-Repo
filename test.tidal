let inverse 1 = 0
    inverse 0 = 1

do
    let pat = "[1 0 1 1 0 1 0 0]"
    d1 $ fast 2 $ gain pat # sound "tt_kick:4" # cut "1"
    d2 $ gain (inverse <$> pat) # sound "tt_fx" # n (choose [9, 7, 9, 10 , 12])
    d3 $ foldEvery [3,4] (0.125 ~>) $ gain pat # sound "tt_clap" # n (irand 8)
    d4 $ gain pat # sound "mlbarkick:2"

